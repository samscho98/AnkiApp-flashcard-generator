  "days": {
    "day_1": {
      "topic": "Grammar Topic 1",
      "words": [
        {
          "tagalog": "ako",
          "english": "I / me",
          "pronunciation": "ah-koh",
          "example": "Ako si Maria.",
          "example_translation": "I am Maria.",
          "rule": "First person pronoun",
          "notes": "Used when referring to oneself",
          "difficulty": 2,
          "tags": [
            "pronoun"
          ]
        },
        {
          "tagalog": "ikaw",
          "english": "you",
          "pronunciation": "ee-kaw",
          "example": "Ikaw si John.",
          "example_translation": "You are John.",
          "rule": "Second person pronoun",
          "notes": "Used to address someone",
          "difficulty": 2,
          "tags": [
            "pronoun"
          ]
        },
        {
          "tagalog": "kumain",
          "english": "ate",
          "pronunciation": "koo-mah-in",
          "example": "Kumain ako ng saging.",
          "example_translation": "I ate a banana.",
          "rule": "Completed aspect",
          "notes": "Used for completed actions",
          "difficulty": 2,
          "tags": [
            "verb"
          ]
        },
        {
          "tagalog": "umiinom",
          "english": "is drinking",
          "pronunciation": "oo-mee-ee-nom",
          "example": "Umiinom siya ng tubig.",
          "example_translation": "He/She is drinking water.",
          "rule": "Incomplete aspect",
          "notes": "Used for ongoing actions",
          "difficulty": 2,
          "tags": [
            "verb"
          ]
        },
        {
          "tagalog": "kakain",
          "english": "will eat",
          "pronunciation": "kah-kah-in",
          "example": "Kakain kami bukas.",
          "example_translation": "We will eat tomorrow.",
          "rule": "Future aspect",
          "notes": "Used for future actions",
          "difficulty": 2,
          "tags": [
            "verb"
          ]
        },
        {
          "tagalog": "hindi",
          "english": "not",
          "pronunciation": "hin-dee",
          "example": "Hindi ako gutom.",
          "example_translation": "I am not hungry.",
          "rule": "Negation word",
          "notes": "Negates verbs and adjectives",
          "difficulty": 2,
          "tags": [
            "negation"
          ]
        }
      ]
    },
